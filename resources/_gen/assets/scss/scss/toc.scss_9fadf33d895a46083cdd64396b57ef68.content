.note h1, .note h2, .note h3, .note h4, .note h5, .note h6 {
  margin: 1rem 0; }

.post-toc {
  margin: 2rem 0; }
  .post-toc .toc-title {
    font-size: 2.6rem; }
  .post-toc ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
    text-indent: -0.8em;
    padding-left: 0.8em;
    line-height: 1.4em;
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); }

