.list .section-title {
  margin: 2rem 0 0 0; }

.list .list-items {
  margin: 0.5rem 0 0 0; }

.note h1, .note h2, .note h3, .note h4, .note h5, .note h6 {
  margin: 2rem 0 0 0; }

.note dt {
  text-align: center;
  font-style: italic;
  font-size: 1.1em; }

.note dt + dd > img {
  display: block;
  margin-left: auto;
  margin-right: auto; }

.post-toc {
  margin: 2rem 0; }
  .post-toc .toc-title {
    font-size: 2.6rem; }
  .post-toc ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); }
    .post-toc ul li {
      text-indent: -0.8em;
      padding-left: 0.8em;
      padding-right: 1em;
      line-height: 1.4em; }

